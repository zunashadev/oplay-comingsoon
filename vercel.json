{
    "version": 2,
    "framework": null,
    "builds": [
        { "src": "/api/index.php", "use": "vercel-php@0.6.0" },
        { "src": "/public/**", "use": "@vercel/static" }
    ],
    "routes": [
        { "src": "/build/(.*)", "dest": "/public/build/$1" },
        { "src": "/storage/(.*)", "dest": "/public/storage/$1" },
        { "src": "/images/(.*)", "dest": "/public/images/$1" },
        { "src": "/fonts/(.*)", "dest": "/public/fonts/$1" },
        { "src": "/icons/(.*)", "dest": "/public/icons/$1" },
        { "src": "/logo/(.*)", "dest": "/public/logo/$1" },
        { "src": "/patterns/(.*)", "dest": "/public/patterns/$1" },
        { "src": "/(.*)", "dest": "/api/index.php" },
        {
            "src": "/(css|js)/(.*)$",
            "dest": "/api/assets.php?type=$1&file=$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "http://oplay-comingsoon.vercel.app/",
        "VITE_APP_NAME": "http://oplay-comingsoon.vercel.app/",
        "APP_KEY": "base64:/rxlphRZVOH/5GJrK04wIWCLHU/eMcRZ9lnY75J8tcQ=",

        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",

        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    }
}
